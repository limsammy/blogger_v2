<div class="articles">
  <h1>All Articles</h1>
  <ul id="articles">
    <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article),
      class: 'waves-light btn-flat', id: "article_#{article.id}" %>
    </li>
    <% end %>
  </ul>
</div>

<div class="new">
    <%= link_to "Create a New Article", new_article_path, class: "waves-light btn" %>
</div>
